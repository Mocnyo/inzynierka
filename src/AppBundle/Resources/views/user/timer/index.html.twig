{% block body %}
    <div class="row">
        <div class="col-sm-12">
            <div class="card shadow mb-4" id="times">
                <div class="card-body">
                    <template v-for="(item, index) in data">
                        <div class="row">
                            <div class="col-sm-6">
                                <input type="text" class="taskdescinput" @change="handleInput(item.id, item.description_task, item.start_time, item.stop_time, item.project.id)"
                                       v-model="item.description_task"/>
                            </div>
                            <div class="col-sm-2 text-center">
                                <li class="nav-item dropdown" style="list-style:none">
                                    <a class="nav-link dropdown nav-color" id="navbarDropdownMenuLink-4"
                                       data-toggle="dropdown"
                                       aria-haspopup="true" aria-expanded="false">
                                        <i class="fas fa-box nav-color project-icon"
                                           v-if="item.project.name == null"></i>
                                        <span class="badge badge-primary" v-else>${ item.project.name }</span>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right dropdown-info project-items"
                                         aria-labelledby="navbarDropdownMenuLink-4">
                                        <template v-for="project in projects">
                                            <a class="dropdown-item" @click="item.project.id = project.id, item.project.name = project.name, handleInput(item.id, item.description_task, item.start_time, item.stop_time, project.id)">
                                                ${ project.name } </a>
                                        </template>
                                    </div>
                                </li>
                            </div>
                            <div class="col-sm-2 text-center d-inline-block">
                                <p class="d-inline-block">
                                    <datetime v-on:close="handleInput(item.id, item.description_task, item.start_time, item.stop_time, item.project.id)" v-model="item.start_time" type="datetime" format="TIME_24_SIMPLE"/>
                                </p> -
                                <p class="d-inline-block">
                                    <datetime v-on:close="handleInput(item.id, item.description_task, item.start_time, item.stop_time, item.project.id)" v-model="item.stop_time" type="datetime" format="TIME_24_SIMPLE"/>
                                </p>
                            </div>
                            <div class="col-sm-1 text-right">
                                <p>${ item.time }</p>
                            </div>
                            <div class="col-sm-1 text-right">
                                <li class="nav-item dropdown" style="list-style:none">
                                    <a class="nav-link dropdown nav-color" id="navbarDropdownMenuLink-4"
                                       data-toggle="dropdown"
                                       aria-haspopup="true" aria-expanded="false">
                                        <i class="fas fa-trash-alt nav-color project-icon"></i>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right dropdown-info project-items"
                                         aria-labelledby="navbarDropdownMenuLink-4">
                                            <a class="dropdown-item" @click="deleteTime(item.id)">
                                                Usu≈Ñ </a>
                                    </div>
                                </li>
                            </div>
                        </div>
                    </template>
                </div>
            </div>
        </div>
    </div>
{% endblock %}